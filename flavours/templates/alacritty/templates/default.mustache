# Base16 {{scheme-name}} - alacritty color config
# {{scheme-author}}
[colors]
draw_bold_text_with_bright_colors = false

[colors.bright]
black = "0x{{base03-hex}}"
blue = "0x{{base04-hex}}"
cyan = "0x{{base0F-hex}}"
green = "0x{{base01-hex}}"
magenta = "0x{{base06-hex}}"
red = "0x{{base09-hex}}"
white = "0x{{base07-hex}}"
yellow = "0x{{base02-hex}}"

[colors.cursor]
cursor = "0x{{base05-hex}}"
text = "0x{{base00-hex}}"

[colors.normal]
black = "0x{{base00-hex}}"
blue = "0x{{base0D-hex}}"
cyan = "0x{{base0C-hex}}"
green = "0x{{base0B-hex}}"
magenta = "0x{{base0E-hex}}"
red = "0x{{base08-hex}}"
white = "0x{{base05-hex}}"
yellow = "0x{{base0A-hex}}"

[colors.primary]
background = "0x{{base00-hex}}"
foreground = "0x{{base05-hex}}"
